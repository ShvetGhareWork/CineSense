# Environment Configuration Example
# Copy this file to .env for development or .env.production for production builds

# Backend API URL
# Development: http://YOUR_LOCAL_IP:5000/api
# Production: https://your-production-backend.com/api
API_URL=http://192.168.0.102:5000/api

# App Environment
NODE_ENV=development

# Enable debug logging (true for development, false for production)
DEBUG_MODE=true
